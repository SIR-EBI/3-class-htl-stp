<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ListDemoController">
   <children>
      <ListView fx:id="listView" layoutX="237.0" layoutY="14.0" prefHeight="372.0" prefWidth="349.0" />
      <TextField fx:id="textField" layoutX="14.0" layoutY="14.0" prefHeight="50.0" prefWidth="210.0" />
      <Button fx:id="addButton" layoutX="14.0" layoutY="79.0" mnemonicParsing="false" onAction="#addButtonPressed" prefHeight="50.0" prefWidth="210.0" text="Add" />
      <Button fx:id="clearButton" layoutX="14.0" layoutY="144.0" mnemonicParsing="false" onAction="#clearButtonPressed" prefHeight="50.0" prefWidth="210.0" text="Clear" />
      <Button fx:id="deleteButton" layoutX="14.0" layoutY="208.0" mnemonicParsing="false" onAction="#deleteButtonPressed" prefHeight="50.0" prefWidth="210.0" text="Delete" />
      <Button fx:id="shuffleButton" layoutX="14.0" layoutY="273.0" mnemonicParsing="false" onAction="#shuffleButtonPressed" prefHeight="50.0" prefWidth="210.0" text="Shuffle" />
      <Button fx:id="sortButton" layoutX="14.0" layoutY="336.0" mnemonicParsing="false" onAction="#sortButtonPressed" prefHeight="50.0" prefWidth="210.0" text="Sort" />
   </children>
</AnchorPane>
