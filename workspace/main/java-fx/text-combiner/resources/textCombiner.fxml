<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TextCombinerController">
   <children>
      <Button fx:id="clearButton" layoutX="120.0" layoutY="161.0" mnemonicParsing="false" onAction="#clearButtonPressed" text="Clear" />
      <TextField fx:id="textFieldOne" layoutX="72.0" layoutY="57.0" />
      <TextField fx:id="textFieldTwo" layoutX="72.0" layoutY="89.0" />
      <Label layoutX="24.0" layoutY="61.0" text="Text 1:" />
      <Label layoutX="24.0" layoutY="93.0" text="Text 2:" />
      <RadioButton fx:id="radioButtonOne" layoutX="21.0" layoutY="24.0" mnemonicParsing="false" selected="true" text="Text1 Text2">
         <toggleGroup>
            <ToggleGroup fx:id="toggleGroup" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="radioButtonTwo" layoutX="166.0" layoutY="24.0" mnemonicParsing="false" text="Text2 Text1" toggleGroup="$toggleGroup" />
      <TextField fx:id="resultTextField" editable="false" layoutX="21.0" layoutY="124.0" prefHeight="25.0" prefWidth="261.0" />
   </children>
</AnchorPane>
